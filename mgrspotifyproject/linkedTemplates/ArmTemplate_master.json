{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mgrspotifyproject"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"SqlServerspotify_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerspotify'"
		},
		"spotify_data_lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'spotify_data_lake'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0208-101826-9jb3y4u8"
		},
		"SqlServerspotify_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-SVRQCC2"
		},
		"SqlServerspotify_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_spotify_db_project"
		},
		"SqlServerspotify_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adf"
		},
		"spotify_data_lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mgrspotifyproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mgrspotifyproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"spotify_data_lake_accountKey": {
						"value": "[parameters('spotify_data_lake_accountKey')]"
					},
					"SqlServerspotify_password": {
						"value": "[parameters('SqlServerspotify_password')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"spotify_data_lake_properties_typeProperties_url": {
						"value": "[parameters('spotify_data_lake_properties_typeProperties_url')]"
					},
					"SqlServerspotify_properties_typeProperties_server": {
						"value": "[parameters('SqlServerspotify_properties_typeProperties_server')]"
					},
					"SqlServerspotify_properties_typeProperties_database": {
						"value": "[parameters('SqlServerspotify_properties_typeProperties_database')]"
					},
					"SqlServerspotify_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerspotify_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}